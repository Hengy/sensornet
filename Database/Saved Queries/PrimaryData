SELECT Node.Command, PrimaryData.Value, PrimaryData.DataLen, PrimaryData.UpdateInterval, PrimaryData.Timestamp, PrimaryData.Type, Bounds.WarningMax, Bounds.WarningMin, Bounds.CriticalMax, Bounds.CriticalMin, ArchiveData.Data
FROM Node
INNER JOIN PrimaryData
ON Node.ID=PrimaryData.ID AND Node.Command=PrimaryData.Command
INNER JOIN Bounds
ON Node.Command=Bounds.Command AND Node.ID=Bounds.ID
INNER JOIN ArchiveData
ON Node.ID=ArchiveData.ID AND Node.Command=ArchiveData.Command;